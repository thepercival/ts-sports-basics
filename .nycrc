{
  "extends": "@istanbuljs/nyc-config-typescript",
  "reporter": ["html", "lcov", "text"],
  "report-dir": "tests/coverage/reports",
  "temp-dir": "tests/coverage/.nyc_output",
  "include": ["src/**/*.ts"],
  "exclude": ["src/index.ts"],
  "extension": [".ts"],
  "clean": true,
  "branches": 95,
  "lines": 95,
  "functions": 95,
  "statements": 95  
}
